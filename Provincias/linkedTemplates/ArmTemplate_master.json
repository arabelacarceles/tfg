{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "Provincias"
		},
		"AzureTableStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"connectionString\"de \"AzureTableStorage1\""
		},
		"AzureFunction1_functionKey": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"functionKey\"de \"AzureFunction1\""
		},
		"RestServiceProvincias_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sedeaplicaciones.minetur.gob.es/ServiciosRESTCarburantes/PreciosCarburantes/Listados/Provincias/"
		},
		"RestServiceProductos_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.precioil.es/"
		},
		"AzureFunction1_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://tfgarabela.azurewebsites.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI de la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas."
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token de SAS para la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Provincias_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureTableStorage1_connectionString": {
						"value": "[parameters('AzureTableStorage1_connectionString')]"
					},
					"AzureFunction1_functionKey": {
						"value": "[parameters('AzureFunction1_functionKey')]"
					},
					"RestServiceProvincias_properties_typeProperties_url": {
						"value": "[parameters('RestServiceProvincias_properties_typeProperties_url')]"
					},
					"RestServiceProductos_properties_typeProperties_url": {
						"value": "[parameters('RestServiceProductos_properties_typeProperties_url')]"
					},
					"AzureFunction1_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('AzureFunction1_properties_typeProperties_functionAppUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}